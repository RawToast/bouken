{
  "name": "bouken",
  "version": "0.2.1b",
  "private": true,
  "dependencies": {
    "bs-fetch": "^0.3.0",
    "bs-webapi": "^0.11.0",
    "rationale": "^0.1.5",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "scripts": {
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf lib",
    "build": "bsb -clean-world -make-world",
    "test": "jest --env=jsdom --testMatch='**/test/*test.{js, re}' --watch",
    "prepare": "npm link bs-platform",
    "coverage": "jest --env=jsdom --testMatch='**/test/*test.{js, re}' --coverage",
    "ci": "jest --env=jsdom --testMatch='**/test/*test.{js, re}' --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.3",
    "parcel": "^1.9.7",
    "reason-react": "^0.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/js/**/*.{js}"
    ]
  }
}

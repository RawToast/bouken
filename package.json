{
  "name": "bouken",
  "version": "0.2.1b",
  "private": true,
  "dependencies": {
    "bs-fetch": "^0.3.0",
    "bs-webapi": "^0.11.0",
    "rationale": "^0.1.5",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "bsb -clean-world && rm -rf lib",
    "bsb": "bsb -make-world",
    "bsb-watch": "bsb -make-world -w",
    "build": "react-scripts build",
    "test": "jest --env=jsdom --testMatch='**/test/*test.{js, re}' --watch",
    "eject": "react-scripts eject",
    "prepare": "npm link bs-platform",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "ci": "react-scripts test --env=jsdom --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.3",
    "reason-react": "^0.5.3",
    "reason-scripts": "0.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{re}"
    ]
  }
}
